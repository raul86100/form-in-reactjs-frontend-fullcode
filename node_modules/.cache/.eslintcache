);\n  let year = date.getFullYear().toString();\n\n  let currentDate = year + \"-\" + mon + \"-\" + day;\n  \n\n  function update() {\n    const apidata= {\n      'firstname':sfirstname,\n      'lastname':slastname,\n      'mobile':smobile,\n      'dob':''+sdob.slice(8,10)+'-' +sdob.slice(5, 7) +'-'+sdob.slice(0, 4),\n      'address':saddress,\n      'email':obj.user.email\n     };\n\n    const url ='http://localhost:5050/api/call/userinfo/update/';\n    console.log(url);\n axios.put(url,apidata)\n      .then((res) => console.log(\"updatedhooooo\",apidata))\n      .catch((error) => console.log(error));\n  }\n\n  return (\n    <div>\n    <img src={logo} className=\"App-logo1\" alt=\"logo\" />\n    <div className=\"add_mainlayout\">\n        \n      <div className=\"innerlayout\">\n        <form\n          onSubmit={() => {\n            window.confirm(\"Are you sure to update\");\n            \n            update();\n            navigate(\"/\");\n          }}\n        >\n          <h1>Edit</h1>\n          <div className=\"addcontent\">\n            <div className=\"part1\">\n              <label>email</label>\n              <div>\n                <label>Firstname</label>\n              </div>\n              <div>\n                <label>Lastname</label>\n              </div>\n              <div>\n                <label>Mobilenumber</label>\n              </div>\n              <div>\n                <label>Date of Birth</label>\n              </div>\n              <div>\n                <label>Address</label>\n              </div>\n              <div><label>    </label></div>\n              <div><label>    </label></div>\n            </div>\n            <div className=\"part2\">\n              <input\n                type=\"text\"\n                placeholder=\"email\"\n                className=\"email\"\n                value={obj.user.email} />\n              <div>\n                <input\n                  type=\"text\"\n                  placeholder=\"firstname\"\n                  onChange={(e) => {\n                    setFirstname(e.target.value);\n                  }}\n                value={sfirstname}/>\n              </div>\n              <div>\n                <input\n                  type=\"text\"\n                  placeholder=\"lastname\"\n                  onChange={(e) => {\n                    setLastname(e.target.value);\n                  }}\n                  value={slastname} />\n              </div>\n              <div>\n                <input\n                  type=\"number\"\n                  placeholder=\"mobilenumber\"\n                  onChange={(e) => {\n                    setMobile(e.target.value);\n                  }}\n                  value={smobile}/>\n              </div>\n              <div>\n                <input\n                  type=\"date\"\n                  min=\"1980-01-01\"\n                  max={currentDate}\n                  onChange={(e) => {\n                    setDob(e.target.value.toString());\n                  }}\n                  value={sdob}/>\n              </div>\n              <div className=\"textarea\">\n                <textarea\n                  type=\"text\"\n                  maxLength=\"50\"\n                  rows=\"3\"\n                  cols=\"30\"\n                  onChange={(e) => {\n                    setAddress(e.target.value);\n                  }}\n                  value={saddress}/>\n              </div>\n            </div>\n          </div>\n          <div className=\"dsubmit\">\n            <input type=\"submit\" value=\"Update\" className=\"updatebtn\"/>\n          </div>\n        </form>\n      </div>\n    </div>\n    </div>\n  );\n}\nexport default Edit;","/home/divum/ragul86100/copies task/first_poc working/src/home.js",[],[],"/home/divum/ragul86100/copies task/first_poc working/src/Add.js",["45","46","47","48","49"],[],"/home/divum/ragul86100/copies task/first_poc working/src/Fetchtable.js",["50","51","52"],[],"import React, { useEffect, useState } from 'react';\nimport axios from 'axios';\n\nimport './App.css';\nimport { Link } from 'react-router-dom';\nimport Popup from \"./component/Popup\";\nimport edit from './edit.png';\nimport dele from './delete.png';\n\n\n\nfunction Fetchtable() {\n    const [rec, setRec] = useState([]);\n\n    const [calldel,setCalldel]=useState(false);\n    const [delkey,setDelkey]=useState(\" \");\n    \n    const fetch = () => {\n        axios.get('http://localhost:5050/api/call/userinfo/')\n            .then(res => setRec(res.data))\n            .catch(err => console.log(err));\n    }\n    useEffect(() => {\n        fetch();\n    }, [!calldel])\n\n    const delmap=(key)=>{\n        setCalldel(true);\n        setDelkey(key); \n    }\n    function del(email) {\n        \n        const url = 'http://localhost:5050/api/call/userinfo/delete/' + email;\n        console.log(url);\n        axios.delete(url)\n            .then(res => console.log(\"deleted\"))\n            .catch(error => console.log(error));\n        \n    }\n\n\n    return (\n       \n       <div> \n       \n        <div className='tabledesign'>\n        <div className='popdel' >\n{calldel && (<Popup\ncloseoption={() => {\nsetCalldel(false);\n}}\ncheckcall={() => {\n\nsetCalldel(false);            \ndel(delkey);\n}}\nmessage={\"Do you want to delete\"}\n className=\"delpop\"/>)}\n\n</div>\n            \n            <table>\n                <thead>\n                    <tr>\n                        <th>Email</th>\n                        <th>FirstName</th>\n                        <th>LastName</th>\n                        <th>Mobile</th>\n                        <th>DOB</th>\n                        <th>Address</th>\n                        <th>Action</th>\n                    </tr>\n                </thead>\n                <tbody>\n                    {\n                        rec.map((user, index) => (\n                            <tr key={index}>\n                                <td>{user.email}</td>\n                                <td>{user.firstname}</td>\n                                <td>{user.lastname}</td>\n                                <td>{user.mobile}</td>\n                                <td>{user.dob}</td>\n                                <td>{user.address}</td>\n                                <td>\n                                    <Link to='/Add' state={{ editobj:{user} }}><button className='homedeletebtn' ><img src={edit} width=\"25px\" height=\"25px\"/></button></Link>\n                                    <button onClick={() => {delmap(user.email)}} className='homeeditbtn' ><img src={dele} width=\"25px\" height=\"25px\"/></button>\n                                </td>\n                            </tr>\n                        ))\n\n                    }\n                </tbody>\n            </table>\n            \n           \n        </div>\n\n\n</div>\n       \n    )\n}\nexport default Fetchtable;\n\n","/home/divum/ragul86100/copies task/first_poc working/src/component/Popup.js",[],[],{"ruleId":"53","severity":1,"message":"54","line":26,"column":6,"nodeType":"55","endLine":26,"endColumn":8,"suggestions":"56"},{"ruleId":"53","severity":1,"message":"57","line":44,"column":6,"nodeType":"55","endLine":44,"endColumn":13,"suggestions":"58"},{"ruleId":"53","severity":1,"message":"59","line":73,"column":6,"nodeType":"55","endLine":73,"endColumn":17,"suggestions":"60"},{"ruleId":"53","severity":1,"message":"61","line":91,"column":6,"nodeType":"55","endLine":91,"endColumn":16,"suggestions":"62"},{"ruleId":"53","severity":1,"message":"63","line":111,"column":6,"nodeType":"55","endLine":111,"endColumn":14,"suggestions":"64"},{"ruleId":"53","severity":1,"message":"65","line":148,"column":6,"nodeType":"55","endLine":148,"endColumn":8,"suggestions":"66"},{"ruleId":"53","severity":1,"message":"67","line":25,"column":9,"nodeType":"68","endLine":25,"endColumn":17},{"ruleId":"69","severity":1,"message":"70","line":85,"column":115,"nodeType":"71","endL